{
  "name": "engine-terminal",
  "version": "0.1.0",
  "description": "The web term version for Engine",
  "keywords": [
    "terminal",
    "jxengine"
  ],
  "author": "jillix <contact@jillix.com>",
  "main": "server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "pty.js": "0.2.6"
  },
  "license": "MIT",
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/jillix/engine-terminal.git"
  },
  "bugs": {
      "url": "https://github.com/jillix/engine-terminal/issues"
  },
  "homepage": "https://github.com/jillix/engine-terminal",

  "composition": {
      "public": "public",
      "flow": [
          {
              "on": "termData",
              "call": "termData"
          }
      ],
      "client": {
          "config": {
              "container": "#terminal"
          },
          "module": [
              "client/index.js",
              "client/text-size.js",
              "client/web-term.js",
              "client/term.js",
              "/jquery.js"
          ],
          "flow": [
              {
                  "on":"termData",
                  "call": "terminal/termData"
              }
          ],
          "styles": ["public/term.css"],
          "markup": []
      }
  }
}
